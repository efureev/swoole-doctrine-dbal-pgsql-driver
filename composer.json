{
  "name": "efureev/swoole-doctrine-dbal-pgsql-driver",
  "description": "The Postgres Swoole 5 driver for Doctrine",
  "license": "MIT",
  "keywords": [
    "symfony",
    "swoole",
    "docrtine",
    "dbal"
  ],
  "authors": [
    {
      "name": "Fureev Eugene",
      "email": "fureev@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "doctrine/dbal": "^4.0",
    "symfony/contracts": "^3.2",
    "symfony/http-kernel": "^7.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.28",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.7.2",
    "vimeo/psalm": "^6.3.0",
    "weirdan/doctrine-psalm-plugin": "^2.10",
    "symfony/dependency-injection": "^7.1"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Swoole\\Packages\\Doctrine\\DBAL\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "psalm": "./vendor/bin/psalm"
  }
}
